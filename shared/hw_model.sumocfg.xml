<?xml version="1.0" encoding="UTF-8"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">

    <input>
        <net-file value="hw_model.net.xml"/>
        <route-files value="hw_model.routes.xml"/>
        <additional-files value="hw_model.vtypes.xml"/>
    </input>

    <time>
        <begin value="0"/>
        <step-length value="1"/>
    </time>

    <processing>
        <time-to-teleport value="-1"/>
        <waiting-time-memory value="100000"/>
    </processing>

    <report>
        <verbose value="true"/>
        <no-step-log value="true"/>
    </report>

    <gui_only>
        <start value="false"/>
        <window-size value="1920,1080"/>
        <window-pos value="1,0"/>
        <tracker-interval value="1"/>
    </gui_only>

</configuration>
